{
  "version": 2,
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors https://discord.com https://*.discord.com https://*.discordsays.com; connect-src *;" },
        { "key": "X-Frame-Options", "value": "ALLOW-FROM https://discord.com" }
      ]
    }
  ]
}


