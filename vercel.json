{
  "version": 2,
  "framework": null,
  "builds": [
    { "src": "client/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors https://discord.com https://*.discord.com;" },
        { "key": "X-Frame-Options", "value": "ALLOW-FROM https://discord.com" }
      ]
    }
  ]
}


